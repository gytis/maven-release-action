{
    "compilerOptions": {
        "outDir": "./built",
        "allowJs": true,
        "target": "es6",
        "module": "CommonJS",
        "noImplicitReturns": true,
        "noEmitOnError": true,
        "noImplicitAny": true,
        "strictNullChecks": true,
        "noImplicitThis": true
    },
    "include": [
        "./src/**/*"
    ]
}